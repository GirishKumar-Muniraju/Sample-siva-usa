{
  "name": "au743-nuc-wrs-user-feedback-lambda",
  "version": "1.0.0",
  "description": "Wrs User Feedback Lambda",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "build": "npm run lint && tsc --project ./ && copyfiles .env ./dist/",
    "coverage": "nyc -r lcov -r text-summary -e .ts -x \"tests/**/**/*.spec.ts\" -x \"src/core/typeorm/migrations/*.ts\" npm run test",
    "test": "cross-env IS_TESTING=true mocha -r ts-node/register 'tests/**/**/*.spec.ts' 'tests/**/*.spec.ts' 'tests/*.spec.ts'",
    "api-server": "npm run build && cd dist && node api-server",
    "dev:debug": "cross-env API_SERVER=true ts-node-dev --inspect=4321 --respawn --exit-child --transpile-only --clear --ignore-watch node_modules src/api-server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "m:run": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/config/migration.config.ts",
    "m:create": "cross-var npm run typeorm migration:create ./src/core/typeorm/migrations/$npm_config_name"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "NextEra Energy",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1490.0",
    "aws-xray-sdk-core": "^3.5.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "lint": "^0.8.19",
    "moment": "^2.29.4",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.125",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.8",
    "@types/chai-spies": "^1.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.2.5",
    "@types/mocha": "^10.0.6",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "aws-sdk-mock": "^5.8.0",
    "babel-jest": "^29.3.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.1.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "jest": "^29.3.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  }
}
